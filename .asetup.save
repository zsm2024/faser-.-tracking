export AtlasVersion="24.0.41"
export AtlasProject="Athena"
export AtlasBaseDir="/cvmfs/atlas.cern.ch/repo/sw/software/24.0"
export ATLAS_RELEASE_BASE="/cvmfs/atlas.cern.ch/repo/sw/software/24.0"
export AtlasBuildBranch="24.0"
export AtlasArea="/cvmfs/atlas.cern.ch/repo/sw/software/24.0/Athena/24.0.41"
export AtlasReleaseType="stable"
export AtlasBuildStamp="2024-04-30T2101"
export SITEROOT="/cvmfs/atlas.cern.ch/repo/sw/software/24.0"
export LCG_RELEASE_BASE="/cvmfs/atlas.cern.ch/repo/sw/software/24.0/sw/lcg/releases"
export G4PATH="/cvmfs/atlas.cern.ch/repo/sw/software/24.0/Geant4"
export TDAQ_RELEASE_BASE="/cvmfs/atlas.cern.ch/repo/sw/tdaq/offline"
source /cvmfs/atlas.cern.ch/repo/sw/software/24.0/Athena/24.0.41/InstallArea/x86_64-el9-gcc13-opt/setup.sh
asetup_status=$?
if [ ${asetup_status} -ne 0 ]; then
    \echo "AtlasSetup(ERROR): sourcing release setup script (/cvmfs/atlas.cern.ch/repo/sw/software/24.0/Athena/24.0.41/InstallArea/x86_64-el9-gcc13-opt/setup.sh) failed"
fi
export TestArea="/afs/cern.ch/work/s/sazhang/public/FASER/alma9_test"
alias_sys_exe emacs
echo $LD_LIBRARY_PATH | egrep "LCG_[^/:]*/curl/" >/dev/null
if [ $? -eq 0 ]; then
    alias_sys_exe_envU git
fi
\expr 1 \* 1 + 1 >/dev/null 2>&1
if [ $? -ne 0 ]; then
    echo -e '\nMaking workaround-alias for expr on this *OLD* machine'; alias_sys_exe expr
fi

